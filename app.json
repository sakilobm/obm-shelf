{
  "expo": {
    "name": "Honlor 2",
    "slug": "honlor2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "honlor2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sakil.honlor2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-admob-native-ads",
        {
          "androidAppId": "ca-app-pub-3194395858987595~7717192541",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511",
          "facebookMediation": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "b.sakil",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6d3c1302-6611-40fa-83d6-49eddbb82aea"
      }
    }
  }
}
